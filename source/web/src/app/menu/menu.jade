div.flex
	md-sidenav.menu-sidenav.md-sidenav-left(md-component-id='left')
		div.md-button-style
			ng-md-icon(icon="{{ sidenav.menuIconIn }}",
					ng-mouseover='sidenav.menuIconIn = "arrow_back"',
					ng-mouseleave='sidenav.menuIconIn = "menu"',
					ng-click='sidenav.leftMenuToggle()')
		md-content.logo-sidenav
			img(src="assets/images/logo/nourriture.svg")
		md-card
			img(src="assets/images/user_dummy/random-user-1.jpg")
			md-card-content
				h2.md-title Profile
				p Random Text
		md-content.item-sidenav
			div(ng-repeat="item in sidenav.sidenavItems")
				md-button. 
							<span class="item_text">{{ item.name }}</span>
							<span class="item_icon"><ng-md-icon icon="{{ item.icon }}"></ng-md-icon></span>